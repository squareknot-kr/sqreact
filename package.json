{
  "name": "sqreact",
  "version": "0.0.8",
  "description": "React components and utilities library",
  "type": "module",
  "main": "./dist/sqreact.umd.cjs",
  "module": "./dist/sqreact.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "import": "./dist/sqreact.js",
      "require": "./dist/sqreact.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css",
    "./css": "./dist/style.css",
    "./styles": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": ["**/*.css"],
  "scripts": {
    "dev": "vite --config vite.config.dev.ts",
    "build": "tsc && vite build",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "keywords": [
    "react",
    "components",
    "pagination",
    "datepicker"
  ],
  "author": "oilater",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kimseonghyeon/sqreact.git"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.6.4"
  }
}